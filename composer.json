    "type": "yii2-extension",
    "license": "MIT",
    "keywords": ["yii", "extension", "jsend"],
    "homepage": "https://github.com/TheRealStart/yii2-jsend-response",
    "support": {
        "email": "alegz@therealstart.com",
        "source": "https://github.com/TheRealStart/yii2-jsend-response"
    },
    "authors": [
        {
            "name": "Alegz",
            "email": "alegz@therealstart.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TRS\\RestResponse\\": ""
        }
    },
    "minimum-stability": "stable",
    "require": {
        "yiisoft/yii2": ">=2.0.6",
        "filsh/yii2-oauth2-server": "~2.2"
    }
